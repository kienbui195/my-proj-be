{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "total_question_theory": {
      "type": "integer",
      "required": false,
      "min": 0,
      "default": 0
    },
    "total_question_simulation": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "tqt_correct": {
      "type": "integer",
      "min": 0,
      "required": false,
      "default": 0
    },
    "tqs_correct": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "avg_time": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "user_role": {
      "type": "enumeration",
      "enum": [
        "admin",
        "user"
      ],
      "default": "user",
      "required": true
    }
  }
}
